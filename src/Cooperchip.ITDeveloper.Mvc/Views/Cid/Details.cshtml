@model Cooperchip.ITDeveloper.Domain.Entities.Cid

@{
    ViewBag.Title = "Detalhes do Genérico: [ " + Model.Diagnostico.ToUpper() + " ]";
}

@{ Layout = null; }


<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title">MedicalManagement-Sys</h4>
        </div>
        <div class="modal-body">
            <div class="form-horizontal">

                <div>
                    <h4> @ViewBag.Title </h4>
                    <hr />
                    <dl class="dl-horizontal">

                        <dt>ID: </dt>
                        <dd>@Html.DisplayFor(model => model.CidInternalId)</dd>

                        <dt>CÓDIGO: </dt>
                        <dd>@Html.DisplayFor(model => model.Codigo)</dd>

                        <dt>DIAGNÓSTICO: </dt>
                        <dd>@Html.DisplayFor(model => model.Diagnostico)</dd>

                    </dl>
                </div>

            </div>
        </div>
        <div class="modal-footer">
            <input type="button" value="Fechar" class="btn btn-default" data-dismiss="modal" />
        </div>

    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
